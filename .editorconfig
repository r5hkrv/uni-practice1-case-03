root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[{node_modules/**,dist/**,package-lock.json}]
charset = unset
end_of_line = unset
indent_size = unset
indent_style = unset
insert_final_newline = unset
trim_trailing_whitespace = unset

[{.editorconfig,.git*,.prettier*,*.json}]
indent_size = 2
indent_style = space

[*.ts]
indent_size = 4
indent_style = tab
